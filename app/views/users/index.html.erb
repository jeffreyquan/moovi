<h1>All Users</h1>
<div class="users container top-buffer">
  <ol>
    <% @users.each do |user| %>
      <li>
        <p><strong><%= user.name %></strong> - <span><%= user.email %> - <%= link_to "Delete", user, :method => 'delete', :data => { :confirm => 'Are you sure?' } %></span></p>

      </li>
    <% end %>
  </ol>
</div>
